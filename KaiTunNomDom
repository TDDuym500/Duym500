    while true do
        for i = 0, 1, 0.01 do
            TextLabel.TextColor3 = Color3.fromHSV(i, 1, 1) -- Chuy·ªÉn m√†u c·∫ßu v·ªìng
            wait(0.05) -- T·ªëc ƒë·ªô ƒë·ªïi m√†u (gi·∫£m ƒë·ªÉ nhanh h∆°n, tƒÉng ƒë·ªÉ ch·∫≠m h∆°n)
