-- C√†i ƒë·∫∑t thu·ªôc t√≠nh cho TextLabel
TextLabel.Parent = ScreenGui
TextLabel.Text = "KaiTun Blox Kid By NomDom Hub"
TextLabel.Size = UDim2.new(0.5, 0, 0.1, 0) -- K√≠ch th∆∞·ªõc ch·ªØ
TextLabel.Position = UDim2.new(0.25, 0, 0.3, 0) -- Hi·ªÉn th·ªã g·∫ßn gi·ªØa m√†n h√¨nh
TextLabel.BackgroundTransparency = 1 -- X√≥a n·ªÅn
TextLabel.TextStrokeTransparency = 0 -- Vi·ªÅn ch·ªØ r√µ r√†ng
TextLabel.TextScaled = true -- T·ª± ch·ªânh k√≠ch th∆∞·ªõc ch·ªØ cho v·ª´a
TextLabel.Font = Enum.Font.SourceSansBold -- Ki·ªÉu ch·ªØ

-- H√†m ƒë·ªïi m√†u c·∫ßu v·ªìng li√™n t·ª•c
